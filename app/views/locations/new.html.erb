<script type="text/javascript">
navigator.geolocation.getCurrentPosition(GetLocation, null, {enableHighAccuracy:true});
function GetLocation(location) {
    alert(location.coords.latitude);
    //alert(location.coords.longitude);
    //alert(location.coords.accuracy);
    document.getElementById('location_longitude').value = location.coords.longitude;
    document.getElementById('location_latitude').value = location.coords.latitude;
    
    var details_string = 'accuracy: ' + location.coords.accuracy;
    document.getElementById('location_details').value = details_string;

}
</script>
<h1>New location</h1>

